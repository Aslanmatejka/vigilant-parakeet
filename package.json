{
  "name": "dogoods-app",
  "version": "1.0.0",
  "description": "A community food sharing platform built with React and Supabase",
  "main": "app.jsx",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "dev": "vite --open",
    "preview": "vite preview",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx",
    "format": "prettier --write .",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:restart": "npx supabase stop && npx supabase start",
    "supabase:reset": "npx supabase db reset",
    "supabase:studio": "npx supabase studio",
    "dev:local": "npm run supabase:start && npm run dev",
    "check:local": "node scripts/check-local.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.4.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-toastify": "^11.0.5",
    "recharts": "^3.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "prettier": "^3.1.0",
    "supabase": "^2.31.8",
    "tailwindcss": "^3.4.0",
    "vite": "^5.4.19",
    "whatwg-fetch": "^3.6.20"
  },
  "keywords": [
    "food-sharing",
    "community",
    "react",
    "supabase"
  ],
  "author": "Aslanmatejka",
  "license": "MIT"
}
